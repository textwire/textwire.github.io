{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "baseUrl": ".",
        "paths": {
            "@/*": ["./docs/.vitepress/theme/*"],
            "@site/*": ["./*"]
        }
    },
    "include": ["docs/.vitepress/**/*"]
}
